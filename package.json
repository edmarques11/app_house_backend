{
  "name": "my-house-backend",
  "type": "module",
  "scripts": {
    "start": "bun src/external/server.ts",
    "build": "prisma generate",
    "postinstall": "prisma generate",
    "start:migrate:prod": "prisma migrate deploy && bun run start",
